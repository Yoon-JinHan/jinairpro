<script type="text/javascript">
var sid = ('prd' == 'prd') ? 'service_1' : 'service_2';
var aid = ('prd' == 'prd') ? 'avail' : 'stg_avail';
var skinId = ('KOR' == 'KOR') ? "nf_skin_eretail" : "nf_skin_eretail_eng";

$(document).ready(function() {
	$.i18n.properties({
		name: ['messages', 'code', 'ibeBooking'],
		path: '/Jinair/jinairfront/www.jinair.com/i18n/messages/',
		mode: 'map',
		language: 'ko_KR',
		langCd: 'KOR',
		callback: initialize
	});
});

initialize = function() {
	$("#btnAgree").click(function() {
		var destinationVal = 'JP';
		var agreeCheck = destinationVal == 'CN' || destinationVal == 'ICNGMPXIY'? true : $("#agree").is(":checked");

		if(!agreeCheck) {
			alertLayer($.i18n.prop('lj.ibe.b2c.rsv.009'));//위 내용을 확인 후 동의하셔야 다음 단계 진행이 가능합니다.
			return;
		}

		if(destinationVal === 'CEBCRKGUM') {
			return window.top.fnSubmit();
		}

		// 인천,김포 <-> 시안 노선 목적지 제주로 변경처리
		if(destinationVal === 'ICNGMPXIY') {
			var cju;
			$.each(window.top.cityAirport.arrivalAirportList, function(i, v) {
				$.each(v.airportList, function(j, o) {
					if(o.iataCityAirportCode == 'CJU'){
						cju = o;
					}
				});
			});

			$(".destination", parent.document).find(".srmy").addClass("setting");
			$(".destination", parent.document).find(".srmy strong").text(cju.cityAirportName);
			$(".destination", parent.document).find(".srmy strong").attr("city", cju.iataCityAirportCode);
			$(".destination", parent.document).find(".srmy strong").attr("country", cju.countryCode);

			// 1여정 출발일 가까운 목,토요일로 변경처리
			var day = toDayOfWeek($('#travelDate1', parent.document).val());
			var dayOfweek = day.getDay();
			var owDate;

			if(dayOfweek < 4){ // 목요일 이전일때
				owDate = day.setDate(day.getDate()+4-dayOfweek);
			}else if(dayOfweek == 7){ // 일요일일때
				owDate = day.setDate(day.getDate()+4);
			}else{
				owDate = day.setDate(day.getDate()+6-dayOfweek);
			}
			$(".date div:eq(0)", parent.document).find(".srmy strong").text(fnGetInputDayLabel(getDateStr(new Date(owDate))));
			$(".date div:eq(0)", parent.document).find(".srmy").removeClass("choice");
			$(".date div:eq(0)", parent.document).find(".srmy").addClass("setting");

			// 왕복여정일 경우
			if($('#origin2', parent.document).val()) {
				// 2여정 기존 출발일 +3일
				$(".date div:eq(1)", parent.document).find(".srmy strong").text(fnGetInputDayLabel(getDateStr(new Date(day.setDate(day.getDate()+3)))));
				$(".date div:eq(1)", parent.document).find(".srmy").removeClass("choice");
				$(".date div:eq(1)", parent.document).find(".srmy").addClass("setting");
			}
			hidePopupLayer();
		}

		var initialBooking = $('#registerform #pnrNumber', parent.document).val() == "";
		if(parent.netfunnelVisible && initialBooking) {
			NetFunnel_Action({ service_id: sid, action_id: aid, skin_id: skinId }, function (ev, ret) {
				$('#registerform', parent.document).submit();
			});
		} else {
			$('#registerform', parent.document).submit();
		}
	});

	//팝업높이조절
	setPopup('113');
};

function toDayOfWeek(date_str) {
	var yyyyMMdd = String(date_str);
	var sYear = yyyyMMdd.substring(0, 4);
	var sMonth = yyyyMMdd.substring(4, 6);
	var sDate = yyyyMMdd.substring(6, 8);
	return new Date(Number(sYear), Number(sMonth) - 1, Number(sDate));
};

function getDateStr(date) {
	var sYear = date.getFullYear();
	var sMonth = date.getMonth() + 1;
	var sDate = date.getDate();

	sMonth = sMonth > 9 ? sMonth : "0" + sMonth;
	sDate  = sDate > 9 ? sDate : "0" + sDate;
	return String(sYear) + String(sMonth) + String(sDate);
}

function fnGetInputDayLabel(convertDate) {
	var week = new Array('('+$.i18n.prop('hom.ibe.rsv.lbl.sun')+')', '('+$.i18n.prop('hom.ibe.rsv.lbl.mon')+')', '('+$.i18n.prop('hom.ibe.rsv.lbl.tue')+')', '('+$.i18n.prop('hom.ibe.rsv.lbl.wed')+')', '('+$.i18n.prop('hom.ibe.rsv.lbl.thu')+')', '('+$.i18n.prop('hom.ibe.rsv.lbl.fri')+')', '('+$.i18n.prop('hom.ibe.rsv.lbl.sat')+')');
	var year  = convertDate.substring(0, 4);
	var month  = convertDate.substring(4, 6);
	var day  = convertDate.substring(6, 8);

	var today = new Date();
	today.setFullYear(year, Number(month) - 1, day);
	var todayLabel = week[today.getDay()];

	return year + "-" + month + "-" + day + todayLabel;
}
</script>

