<script type="text/javascript">
$(document).ready(function() {
	setPopup(113);

	var prevCountry = 'KOR';

	var localeSets = {
		'KOR': {foPop: 'KR', foLangCd: 'KOR', foLang: 'ko', foLangCountry: 'ko_KR'},
		'JPN': {foPop: 'JP', foLangCd: 'JPN', foLang: 'ja', foLangCountry: 'ja_JP'},
		'CHN': {foPop: 'CN', foLangCd: 'CHN', foLang: 'zh-Hans', foLangCountry: 'zh_CN'},
		'HKG': {foPop: 'HK', foLangCd: 'CHT', foLang: 'zh-Hant', foLangCountry: 'zh_TW'},
		'MAC': {foPop: 'HK', foLangCd: 'CHT', foLang: 'zh-Hant', foLangCountry: 'zh_TW'},
		'TWN': {foPop: 'TW', foLangCd: 'CHT', foLang: 'zh-Hant', foLangCountry: 'zh_TW'},
		'THA': {foPop: 'TH', foLangCd: 'ENG', foLang: 'en', foLangCountry: 'en_US'},
		'VNM': {foPop: 'VN', foLangCd: 'ENG', foLang: 'en', foLangCountry: 'en_US'},
		'MYS': {foPop: 'MY', foLangCd: 'ENG', foLang: 'en', foLangCountry: 'en_US'},
		'PHL': {foPop: 'US', foLangCd: 'ENG', foLang: 'en', foLangCountry: 'en_US'},
		'LAO': {foPop: 'US', foLangCd: 'ENG', foLang: 'en', foLangCountry: 'en_US'},
		'USA': {foPop: 'US', foLangCd: 'ENG', foLang: 'en', foLangCountry: 'en_US'},
		'AUS': {foPop: 'US', foLangCd: 'ENG', foLang: 'en', foLangCountry: 'en_US'}
	};

	$('#btnConfirm').click(function() {
		var foCountry = $('[role="locale-selector"]').val();
		var foPop = localeSets[foCountry].foPop;
		var foLangCd = localeSets[foCountry].foLangCd;
		var foLang = localeSets[foCountry].foLang;
		var foLangCountry = localeSets[foCountry].foLangCountry;

		// remove cookie
		var cookies = (!!Cookies && 'function' == typeof Cookies.noConflict) ? Cookies.noConflict() : Cookies;
		if(cookies) {
			cookies.set('foCountry', foCountry, {domain : '.jinair.com', path : '/', expires: 0});
			cookies.set('foPop', foPop, {domain : '.jinair.com', path : '/', expires: 0});
			cookies.set('foLangCd', foLangCd, {domain : '.jinair.com', path : '/', expires: 0});
			cookies.set('foLang', foLang, {domain : '.jinair.com', path : '/', expires: 0});
			cookies.set('foLangCountry', foLangCountry, {domain : '.jinair.com', path : '/', expires: 0});
		}

		var bssl = document.location.protocol.indexOf("https") > -1;
		if(bssl) {
			cookies.set('foCountry', foCountry, {path : '/', expires: 365,secure :  true, samesite : 'none'});
			cookies.set('foPop', foPop, {path : '/', expires: 365,secure : true, samesite : 'none'});
			cookies.set('foLangCd', foLangCd, {path : '/', expires: 365,secure :  true, samesite : 'none'});
			cookies.set('foLang', foLang, {path : '/', expires: 365,secure :  true, samesite : 'none'});
			cookies.set('foLangCountry', foLangCountry, {path : '/', expires: 365,secure :  true, samesite : 'none'});
		} else {
			cookies.set('foCountry', foCountry, {path : '/', expires: 365});
			cookies.set('foPop', foPop, {path : '/', expires: 365});
			cookies.set('foLangCd', foLangCd, {path : '/', expires: 365});
			cookies.set('foLang', foLang, {path : '/', expires: 365});
			cookies.set('foLangCountry', foLangCountry, {path : '/', expires: 365});
		}

		doGateSetting(foCountry, foLang);	// mobileApp 연계

		var newCountry = cookies.get('foCountry') !== prevCountry;
		if(newCountry) {
			if(-1 < 'https://www.jinair.com/booking/index'.indexOf('/booking/')) {
				parent.location.href = '/';
			} else {
				parent.location.reload();
			}
		} else {
			parent.location.reload();
		}
	});
});
</script>

