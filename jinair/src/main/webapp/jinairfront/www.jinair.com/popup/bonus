<script>
var cpnListString = '[]';
var embrgListString = '[]';

function jsSelectCoupon(){
	var cpnListJson = ( cpnListString != null && cpnListString !='' ) ? JSON.parse(cpnListString) : '';
	var embrgListJson = ( embrgListString != null && embrgListString !='' ) ? JSON.parse(embrgListString) : '';
	
	var cpnno = $(':input:radio[name=cpnno]:checked').val();
	
	if ( !cpnno ){
		alertLayer($.i18n.prop('lj.ibe.b2c.rsv.065'))//쿠폰을 선택해주세요.
		return false;
	} else {
		var rtnCpn ={}
		var rtnEmbrg = [];
		
		$(cpnListJson).each(function(i){
			if ( $(this)[0].cpnNo==cpnno ){
				rtnCpn  = $(this)[0];
			}
		});
		
		$(embrgListJson).each(function(i){
			if ( $(this)[0].cpnNo==cpnno ){
				rtnEmbrg.push($(this)[0]);
			}
		});
		
		parent.registerform.cpnNo.value =cpnno;
		
		parent.bonusCoupon = rtnCpn;
		parent.bonusCouponEmbargo = rtnEmbrg;
		parent.setCoupon();
		parent.hidePopupLayer();
	}

}
$(document).ready(function() {
	/*다국어처리*/
	$.i18n.properties({
		name	: ['messages', 'code','ibeBooking'],
		path	: '../i18n/messages/',
		mode	: 'map',
		language: 'ko_KR',
	    langCd	: 'KOR'
	});
	
	//팝업높이조절
	setPopup('113');
});

</script>

