function promotionInfoPopup(popupId) {
	$('#popCont').css('display', 'none');
	$('#popCont' + popupId).show();
	$('#popCont' + popupId).prop('id', 'popCont');
	//팝업높이조절
	setPopup('113');
}
</script>

