<script type="text/javascript">
var country	= '';

$(window).ready(function() {
	country	= JSON.parse('[{"ctrNmKor":"한국","ctrNmEng":"REPUBLIC OF KOREA","ctrPop":"KR","ctrCd":"KOR","langList":[{"stSeq":10,"langNm":"한국어","langCd":"KOR|ko|ko_KR"}]},{"ctrNmKor":"일본","ctrNmEng":"JAPAN","ctrPop":"JP","ctrCd":"JPN","langList":[{"stSeq":30,"langNm":"日本語","langCd":"JPN|ja|ja_JP"}]},{"ctrNmKor":"대만","ctrNmEng":"TAIWAN","ctrPop":"TW","ctrCd":"TWN","langList":[{"stSeq":50,"langNm":"中文繁體","langCd":"CHT|zh-Hant|zh_TW"}]},{"ctrNmKor":"라오스","ctrNmEng":"LAOS","ctrPop":"LA","ctrCd":"LAO","langList":[{"stSeq":20,"langNm":"ENGLISH","langCd":"ENG|en|en_US"}]},{"ctrNmKor":"마카오","ctrNmEng":"MACAO","ctrPop":"HK","ctrCd":"MAC","langList":[{"stSeq":50,"langNm":"中文繁體","langCd":"CHT|zh-Hant|zh_TW"}]},{"ctrNmKor":"말레이시아","ctrNmEng":"MALAYSIA","ctrPop":"MY","ctrCd":"MYS","langList":[{"stSeq":20,"langNm":"ENGLISH","langCd":"ENG|en|en_US"}]},{"ctrNmKor":"미국","ctrNmEng":"U S A","ctrPop":"US","ctrCd":"USA","langList":[{"stSeq":20,"langNm":"ENGLISH","langCd":"ENG|en|en_US"}]},{"ctrNmKor":"베트남","ctrNmEng":"VIET NAM","ctrPop":"VN","ctrCd":"VNM","langList":[{"stSeq":20,"langNm":"ENGLISH","langCd":"ENG|en|en_US"}]},{"ctrNmKor":"중국","ctrNmEng":"MAINLAND CHINA","ctrPop":"CN","ctrCd":"CHN","langList":[{"stSeq":40,"langNm":"简体","langCd":"CHN|zh-Hans|zh_CN"}]},{"ctrNmKor":"태국","ctrNmEng":"THAILAND","ctrPop":"TH","ctrCd":"THA","langList":[{"stSeq":20,"langNm":"ENGLISH","langCd":"ENG|en|en_US"}]},{"ctrNmKor":"필리핀","ctrNmEng":"PHILIPPINES","ctrPop":"US","ctrCd":"PHL","langList":[{"stSeq":20,"langNm":"ENGLISH","langCd":"ENG|en|en_US"}]},{"ctrNmKor":"호주","ctrNmEng":"AUSTRALIA","ctrPop":"US","ctrCd":"AUS","langList":[{"stSeq":20,"langNm":"ENGLISH","langCd":"ENG|en|en_US"}]},{"ctrNmKor":"홍콩","ctrNmEng":"HONG KONG","ctrPop":"HK","ctrCd":"HKG","langList":[{"stSeq":50,"langNm":"中文繁體","langCd":"CHT|zh-Hant|zh_TW"}]}]');

	if ('KOR' == 'KOR') {
		// 한국을 최상단으로, 나머지는 한글 국가명 ASC
		var	korJson	= {};
		var	etcJson	= [];
		for (var i = 0; i < country.length; i++) {
			if ('KOR' == country[i].ctrCd) {
				korJson	= country[i];
			} else {
				etcJson.push(country[i]);
			}
		}

		etcJson.sort(customSortKor);

		country	= [];
		country[0]	= korJson;

		for (var i = 0; i < etcJson.length; i++) {
			country[i + 1]	= etcJson[i];
		}
	} else {
		// 영문 국가명 ASC
		country.sort(customSortEng);
	}

	$('#region').change(function() {
		var	ctrCd	= $('#region option:selected').val();
		var	langCd	= Cookies.get('foLangCd') + '|' + Cookies.get('foLang') + '|' + Cookies.get('foLangCountry');

		for (var i = 0; i < country.length; i++) {
			if (ctrCd == country[i].ctrCd + '|' + country[i].ctrPop) {
				$('#lang').empty();

				var	orderedLangList	= country[i].langList;
				orderedLangList.sort(customSortSeq);

				for (var j = 0; j < orderedLangList.length; j++) {
					var	selected	= "";

					if (langCd == orderedLangList[j].langCd) {
						selected	= ' selected="selected"';
					}

					$('#lang').append('<option value="' + orderedLangList[j].langCd + '"' + selected + '>' + orderedLangList[j].langNm + '</option>');
				}
			}
		}
	});

	$('#lang').change(function() {
		$('#btnConfirm').focus();
	});

	$('#btnConfirm').click(function() {
		var	ctrCd	= $('#region option:selected').val();
		var	langCd	= $('#lang option:selected').val();
		var	ctr		= ctrCd.split('|');
		var	lang	= langCd.split('|');
		var	cookies	= ('function' == typeof Cookies.noConflict) ? Cookies.noConflict() : Cookies;

		// remove cookie
		if(cookies) {
			cookies.set('foCountry', '', {domain : 'loaclhost', path : '/', expires: 0 });
			cookies.set('foPop', '', {domain : 'loaclhost', path : '/', expires: 0});
			cookies.set('foLangCd', '', {domain : 'loaclhost', path : '/', expires: 0});
			cookies.set('foLang', '', {domain : 'loaclhost', path : '/', expires: 0});
			cookies.set('foLangCountry', '', {domain : 'loaclhost', path : '/', expires: 0});
		}
		
		var bssl = document.location.protocol.indexOf("https") >-1;
		if(bssl) {
			cookies.set('foCountry', ctr[0], {path : '/', expires: 365,secure :  true, samesite : 'none'});
			cookies.set('foPop', ctr[1], {path : '/', expires: 365,secure : true, samesite : 'none'});
			cookies.set('foLangCd', lang[0], {path : '/', expires: 365,secure :  true, samesite : 'none'});
			cookies.set('foLang', lang[1], {path : '/', expires: 365,secure :  true, samesite : 'none'});
			cookies.set('foLangCountry', lang[2], {path : '/', expires: 365,secure :  true, samesite : 'none'});
		} else {
			cookies.set('foCountry', ctr[0], {path : '/', expires: 365});
			cookies.set('foPop', ctr[1], {path : '/', expires: 365});
			cookies.set('foLangCd', lang[0], {path : '/', expires: 365});
			cookies.set('foLang', lang[1], {path : '/', expires: 365});
			cookies.set('foLangCountry', lang[2], {path : '/', expires: 365});
		}
		doGateSetting(ctr[0], lang[1]);	// mobileApp 연계

		var returnUrl = '/Jinair/jinairfront/www.jinair.com/booking/index.jsp';
		location.href = returnUrl.replace(/&amp;/g, '&');
	});

	// fill region select box
	$('#region').empty();

	for (var i = 0; i < country.length; i++) {
		var	selected	= "";

		if('KOR' == country[i].ctrCd) {
			selected	= ' selected="selected"';
		}

		$('#region').append('<option value="' + country[i].ctrCd + '|' + country[i].ctrPop + '"' + selected + '>' + country[i].ctrNmKor + ' (' + country[i].ctrNmEng + ')' + '</option>');
	}

	$('#region').trigger('change');
});

function customSortKor(a, b) {
	if (a.ctrNmKor == b.ctrNmKor) {
		return 0;
	}

	return (a.ctrNmKor > b.ctrNmKor) ? 1 : -1;
}

function customSortEng(a, b) {
	if (a.ctrNmEng == b.ctrNmEng) {
		return 0;
	}

	return (a.ctrNmEng > b.ctrNmEng) ? 1 : -1;
}

function customSortSeq(a, b) {
	if (a.stSeq == b.stSeq) {
		return 0;
	}

	return (a.stSeq > b.stSeq) ? 1 : -1;
}
