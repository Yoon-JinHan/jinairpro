<script>
window.onload=function() {
	var $tabButtonItem = $('#tabButton li'),
		$tabSelect = $('#tabSelect'),
		$tabContents = $('.cntsBody > .cont'),
		activeClass = 'on';

	$tabButtonItem.first().addClass(activeClass).attr('title', '선택됨');
	$tabContents.not(':first').hide();

	// PC, 테블릿
	$tabButtonItem.find('a').on('click', function(e) {
		var target = $(this).attr('href');

		$tabButtonItem.removeClass(activeClass).attr('title', '');
		$(this).parent().addClass(activeClass).attr('title', '선택됨');
		$tabSelect.val(target);
		$tabContents.hide();
		$(target).show();
		e.preventDefault();
	});

	// 모바일
	$tabSelect.on('change', function() {
		var target = $(this).val(),
			targetSelectNum = $(this).prop('selectedIndex');

		$tabButtonItem.removeClass(activeClass).attr('title', '');
		$tabButtonItem.eq(targetSelectNum).addClass(activeClass).attr('title', '선택됨');
		$tabContents.hide();
		$(target).show();
	});
};
</script>

